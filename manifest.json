{
    "name": "Hide Naver Webtoon",
    "description": "Hide Naver webtoon",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "/images/hnw16.png",
        "32": "/images/hnw32.png",
        "48": "/images/hnw48.png",
        "128": "/images/hnw128.png"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["https://comic.naver.com/webtoon/*"],
            "js": ["node_modules/@webcomponents/custom-elements/custom-elements.min.js", "class/HideUI.js", "content-script.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["/images/hide.png"],
            "matches": ["https://comic.naver.com/*"],
            "extension_ids": ["nnleeadcdkljjgjoaclofphnjdnaaiek"]
        }
    ]
}